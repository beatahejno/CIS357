<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="350.0" style="-fx-background-color: grey;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ViewBooksPage">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="12.0" layoutY="-1.0" prefHeight="434.0" prefWidth="327.0">
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <padding>
                <Insets bottom="10.0" top="10.0" />
            </padding>
         <children>
            <Label text="Issued Books Table" textFill="WHITE" underline="true">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TableView fx:id="tblTransactions" prefHeight="385.0" prefWidth="327.0">
              <columns>
                <TableColumn fx:id="bookID" prefWidth="69.0" text="Book ID" />
                <TableColumn fx:id="userID" prefWidth="70.0" text="User ID" />
                  <TableColumn fx:id="issueDate" prefWidth="182.0" text="Issue Date" />
              </columns>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </TableView>
         </children>
        </VBox>
    </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</AnchorPane>
